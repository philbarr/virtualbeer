{
    com.badlogic.gdx.graphics.g2d.BitmapFont: {
        liberation-font: { file: liberation32.fnt },
    },
    com.badlogic.gdx.scenes.scene2d.ui.Label$LabelStyle: {
	  main: {
	    font: liberation-font,
	  },
	},
	com.badlogic.gdx.scenes.scene2d.ui.Button$ButtonStyle: {
		flash: {
			up: btn_flash_off,
			checked: btn_flash_on,
		},
		camera_small: {
			up: btn_camera_small_on,
			checked: btn_camera_small_off,
		},
		camera_large: {
			up: btn_camera_large_on,
			checked: btn_camera_large_off,
		},
		menu: {
			up: btn_menu,
		},
		instructions: {
			up: btn_instructions,
		},
		exit: {
			up: btn_exit,
		},
	},
}